try {
	(function() {
        var args = {
            article_name: "",
            search: "",
            category_name: "",
            keywords: "",
            site_url: ""
        };

        try {
            args.article_name = "";
            args.search = "";
            args.category_name = "";
            args.site_url = encodeURIComponent("");
        } catch(e) { }

		try{
			args.keywords = "";
		} catch(e) {  }

		if(!args.keywords || args.keywords.length == "") {
			for(element in document.getElementsByTagName("meta")) {
				if(document.getElementsByTagName("meta")[element].name && document.getElementsByTagName("meta")[element].name == "keywords") {
					args.keywords = document.getElementsByTagName("meta")[element].content;
				}
			}
			args.keywords = encodeURIComponent(args.keywords).replace(/\%20/g,'+');
		}

        if(typeof window.exelateTags == "undefined") {
            window.exelateTags = [];
        }

        window.exelateTags.push({
            "src": "http://loadus.exelator.com/load/?p=258&g=137&articlename="+args.article_name+"&search="+args.search+"&Vertical=home&sitecat=home&category="+args.category_name+"&sitename=inhabitat&keywords="+args.keywords+"&pageurl="+args.site_url,
